
public class Kazan
{
    private int numberOfKorgools;
    
 
    public Kazan()
    {
        numberOfKorgools = 0;
    }

    public int getNumberOfKorgools()
    {
        return numberOfKorgools;
    }
    
    public void addKorgools(int number)
    {
        numberOfKorgools += number;
    }
}
