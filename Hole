
/**
 * 在这里给出对类 Hole 的描述。
 * 
 * @作者（你的名字）
 * @版本（一个版本号或者一个日期）
 */
public class Hole
{
    private int numberOfKorgools;

    public Hole()
    {
        numberOfKorgools = 9;
    }

    public int getNumberOfKorgools()
    {
        return numberOfKorgools;
    }
    
    public void addKorgools()
    {
        numberOfKorgools++;
    }
    
    public void deleteKorgools(int number)
    {
        numberOfKorgools -= number;
    }
}
